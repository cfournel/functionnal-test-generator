{
    "name":         "huitiemesens/functional-test-generator",
    "description":  "String transliterator",
    "keywords":     ["functional-test-generator", "generator"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Charles-Antoine FOURNEL, Huitiemesens",
            "homepage": "http://www.huitiemesens.fr/"
        },
        {
            "name": "Community contributions",
            "homepage": "https://github.com/liip/LiipFunctionalTestBundle/contributors"
        }
    ],

    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.3|~3.0",
        "symfony/console": "~2.1|~3.0",
        "symfony/browser-kit": "~2.3|~3.0",
        "symfony/validator": "~2.5|~3.0",
        "doctrine/common": "~2.0"
    },

    "require-dev":{
        "doctrine/orm": "~2.4",
        "doctrine/doctrine-bundle": "~1.2",
        "phpunit/phpunit": "~4.1",
        "liip/functional-test-bundle": "^1.2",
        "propel/propel1": "1.6.*"
    },

    "autoload": {
        "psr-0": {
            "huitiemesens\\functional-test-generator": "src/"
        }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
